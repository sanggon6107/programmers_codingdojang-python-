    "    s, res = 'aAbBcCdDeEfFgGhHiIjJkKlLmMnNoOpPqQrRsStTuUvVwWxXyYzZ', ''\n",
    "    for M in inp :\n",
    "        if M in s : res += s[(s.index(M)+2*n)%52]\n",
    "        else : res += M\n",
    "    return res\n",
    "INP = str(input(\"INPUT : \"))\n",
    "N = int(input(\"N : \"))\n",
    "print(enig(INP, N))"
