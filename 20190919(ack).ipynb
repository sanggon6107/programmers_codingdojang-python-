    "from __future__ import print_function, division\n",
    "\n",
    "\n",
    "def ackermann(m, n):\n",
    "    if m == 0:\n",
    "        return n+1\n",
    "    if n == 0:\n",
    "        return ackermann(m-1, 1)\n",
    "    return ackermann(m-1, ackermann(m, n-1))\n",
    "\n",
    "\n",
    "# 파이썬에서는 for나 while의 재귀 구문을 이용하지 않고서도 재귀 함수를 정의할 수 있다!\n",
    "\n",
    "print(ackermann(3, 3))\n",
    "\n",
    "\n"
   ]
